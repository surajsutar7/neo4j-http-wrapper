{
  "name": "neo4j-http-wrapper",
  "version": "0.1.0",
  "description": "HTTP wrapper for Neo4j with API key authentication, connection pooling, health check, and Swagger docs",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "ts-node src/index.ts",
    "build": "tsup src/index.ts --dts",
    "start": "node dist/index.js",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "neo4j",
    "neo4j-driver",
    "cypher",
    "graph-database",
    "http-api"
  ],
  "author": "Suraj Sutar",
  "license": "MIT",
  "dependencies": {
    "dotenv": "^16.4.0",
    "express": "^4.19.2",
    "neo4j-driver": "^5.15.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "@types/node": "^25.1.0",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "ts-node": "^10.9.2",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3"
  }
}
